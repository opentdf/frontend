{
  "name": "abacus",
  "version": "2.0.0",
  "license": "BSD-3-Clause-Clear",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:prod": "tsc && vite build",
    "test:playwright": "playwright test --config=tests/e2e/playwright.config.ts",
    "test:playwright-debug": "playwright test --debug --config=tests/e2e/playwright.config.ts",
    "test:playwright-ci": "start-server-and-test start http://localhost:3000 test:playwright"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@react-keycloak/web": "^3.4.0",
    "antd": "^4.23.5",
    "axios": "^1.1.2",
    "pullstate": "^1.24.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "react-toastify": "^9.0.8"
  },
  "homepage": ".",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 18,
        "functions": 10,
        "lines": 15,
        "statements": 10
      }
    }
  },
  "browserslist": {
    "production": [
      "last 2 chrome version",
      "last 2 firefox version",
      "last 2 edge version",
      "last 2 and_chr version",
      "last 2 ios_saf version",
      "last 2 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^2.1.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
