    "istanbul-lib-report": "^3.0.1",
    "istanbul-merge": "^2.0.0",
    "istanbul-reports": "^3.1.6",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "start-server-and-test": "^2.0.3",
    "ts-jest": "^29.1.2",
    "vite": "^5.0.12",
    "vite-plugin-istanbul": "^5.0.0"
  }
}
