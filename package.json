    "@types/jest": "~29.5.11",
    "@types/react": "~18.2.48",
    "@types/react-dom": "~18.2.18",
    "@types/react-redux": "^7.1.33",
    "antd": "^4.24.15",
    "axios": "^1.6.5",
    "keycloak-js": "^23.0.4",
    "oidc-client": "~1.11.5",
    "pullstate": "^1.25.0",
